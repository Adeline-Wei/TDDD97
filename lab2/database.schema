CREATE TABLE IF NOT EXISTS Users (uid INTEGER PRIMARY KEY AUTOINCREMENT, email TEXT NOT NULL, password TEXT NOT NULL, firstname TEXT NOT NULL, familyname TEXT NOT NULL, gender TEXT NOT NULL, city TEXT NOT NULL, country TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS Messages (mid INTEGER PRIMARY KEY AUTOINCREMENT, uid INTEGER NOT NULL, from_uid INTEGER NOT NULL, content TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS Logins (lid INTEGER PRIMARY KEY AUTOINCREMENT, uid INTEGER, email TEXT, token TEXT NOT NULL);